<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.mapper.Sy_EmpAreaPowerDao">

	<insert id="addEmpAreaPower" parameterType="sy_EmpAreaPower">
		INSERT INTO sy_empareapower VALUES(#{empID},#{areaID})
	</insert>
	
	<delete id="deleteEmpAreaPower" parameterType="int">
		delete from sy_empareapower where empID=#{empID}
	</delete>
</mapper>